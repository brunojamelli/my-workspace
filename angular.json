    "minha-biblioteca": {
      "projectType": "library",
      "root": "projects/minha-biblioteca",
      "sourceRoot": "projects/minha-biblioteca/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
